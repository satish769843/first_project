<table class="table">
    <thead class="thead-light">
      <tr>
        <th scope="col">Title</th>
        <th scope="col">Short Description</th>
        <th scope="col">Long Description</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      
          <% postData.forEach((e)=>{ %>
            <tr>
            <td><%=e.title %></td>
            <td><%=e.sDesc %></td>
            <td><%= e.lDesc %></td>
            <td>
                <a class="btn btn-primary btn-xs" href='/admin/edit/<%=e._id %>'><i class="fa fa-edit"></i></a>
                <a class="btn btn-danger btn-xs" href='/admin/delete/<%=e.id %>'><i class="fa fa-trash"></i></a>
            </td>
        </tr>
        <%  }) %>
    </tbody>
  </table>